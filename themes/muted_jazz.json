{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Muted Jazz",
  "author": "jth",
  "themes": [
    {
      "name": "Muted Jazz",
      "appearance": "dark",
      "style": {
        "background.appearance": "opaque",
        "background": "#1D2022",
        "surface.background": "#1D2022CC",
        "border": "#2B2B2B",
        "border.variant": "#3B3B3B",
        "border.focused": "#3E809B",
        "element.background": "#2A2E31",
        "element.hover": "#31383A",
        "element.active": "#384146",
        "element.selected": "#384146",
        "text": "#D2D4D4",
        "text.muted": "#9A9C9C",
        "editor.background": "#1D2022",
        "editor.foreground": "#D2D4D4",
        "editor.active_line.background": "#262A2C",
        "editor.gutter.background": "#1D2022",
        "editor.line_number": "#6E7072",
        "editor.active_line_number": "#CCCCCC",
        "editor.indent_guide": "#383A3C",
        "editor.indent_guide_active": "#5A5C5E",
        "scrollbar.thumb.background": "#79797966",
        "scrollbar.thumb.hover_background": "#646464B3",
        "scrollbar.thumb.active_background": "#BFBFBF66",
        "status_bar.background": "#181A1B",
        "title_bar.background": "#181A1B",
        "tab.active_background": "#1D2022",
        "tab.inactive_background": "#181A1B",
        "panel.background": "#181A1B",
        "tooltip.background": "#262A2C",
        "tooltip.foreground": "#D2D4D4",
        "diagnostic.info.background": "#262A2C",
        "diagnostic.info.foreground": "#D2D4D4",
        "diagnostic.warning.background": "#2C2824",
        "diagnostic.warning.foreground": "#E0C188",
        "diagnostic.error.background": "#2A2222",
        "diagnostic.error.foreground": "#A66A6A",


        "syntax": {
          "comment":               { "color": "#6C756A" },
          "comment.doc":           { "color": "#6C756A" },
          "keyword":               { "color": "#3E809B" },
          "operator":              { "color": "#D2D4D4" },
          "boolean":               { "color": "#5E9CB0" },
          "constant":              { "color": "#4E9A91" },
          "number":                { "color": "#E0C188" },
          "string":                { "color": "#E0C188" },
          "string.escape":         { "color": "#D2A46A" },
          "string.regex":          { "color": "#A66A6A" },
          "string.special":        { "color": "#A66A6A" },
          "text.literal":          { "color": "#E0C188" },
          "function":              { "color": "#76A8BF" },
          "function.method":       { "color": "#76A8BF" },
          "function.macro":        { "color": "#9A7AD4" },
          "constructor":           { "color": "#76A8BF" },
          "type":                  { "color": "#4E9A91" },
          "type.builtin":          { "color": "#4E9A91" },
          "type.enum":             { "color": "#5BAEA0" },
          "type.parameter":        { "color": "#6CB4A8" },
          "variable":              { "color": "#D2D4D4" },
          "variable.constant":     { "color": "#E0C188" },
          "variable.global":       { "color": "#C6B888" },
          "variable.parameter":    { "color": "#D2B480" },
          "variable.static":       { "color": "#C8C8B0" },
          "variable.special":      { "color": "#B4C8C8" },
          "property":              { "color": "#88BFC0" },
          "attribute":             { "color": "#88BFC0" },
          "enum":                  { "color": "#5BAEA0" },
          "enumMember":            { "color": "#76B8B8" },
          "interface":             { "color": "#4E9A91" },
          "namespace":             { "color": "#3E809B" },
          "macro":                 { "color": "#9A7AD4" },
          "modifier":              { "color": "#A66A6A" },
          "parameter":             { "color": "#D2B480" },
          "label":                 { "color": "#D2B480" },
          "preproc":               { "color": "#A66A6A" },
          "punctuation":           { "color": "#C6C6C6" },
          "punctuation.bracket":   { "color": "#C6C6C6" },
          "punctuation.delimiter": { "color": "#C6C6C6" },
          "punctuation.special":   { "color": "#D2D4D4" },
          "struct":                { "color": "#4E9A91" },
          "tag":                   { "color": "#3E809B" },
          "embedded":              { "color": "#D2D4D4" },
          "emphasis.strong":       { "color": "#3E809B", "font_weight": 700 }
        }
      }
    }
  ]
}
